{
    "name": "asset-pipe-css-writer",
    "version": "1.0.0-alpha.1",
    "author": {
        "name": "Richard Walker",
        "email": "digitalsadhu@gmail.com"
    },
    "description": "CSS asset feed writer",
    "main": "./lib/writer.js",
    "repository": {
        "type": "git",
        "url": "git@github.com:asset-pipe/asset-pipe-css-writer.git"
    },
    "keywords": ["css", "assets"],
